                android:id="@+id/spinnerHighFt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/FEET" />
            <Spinner
                android:id="@+id/spinnerHighInch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/INCH" />
            <Spinner
                android:id="@+id/spinnerHighFrac"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/FRAC" />
        </TableRow>
        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textViewLow" />
