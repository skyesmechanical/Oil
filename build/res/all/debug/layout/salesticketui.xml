                android:id="@+id/spinnerLowFt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/FEET" />
            <Spinner
                android:id="@+id/spinnerLowInch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/INCH" />
            <Spinner
                android:id="@+id/spinnerLowFrac"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/FRAC" />
