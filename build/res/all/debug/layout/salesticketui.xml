                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textViewTemp" />
            <Spinner
                android:id="@+id/spinnerObTemp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/Temp" />
        </TableRow>
