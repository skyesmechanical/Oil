        		android:id="@+id/lblDate"
        		android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		android:text="Current Date: "
        		android:textAppearance="?android:attr/textAppearanceSmall" />
		    <TextView
        		android:id="@+id/tvDate"
        		android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		android:text=""
        		android:textAppearance="?android:attr/textAppearanceSmall" />
