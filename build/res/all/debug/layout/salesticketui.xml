            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lowTemp" />
            <Spinner
                android:id="@+id/spinnerLowTemp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/Temp" />
